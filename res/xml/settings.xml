<?xml version="1.0" encoding="utf-8"?>
<!-- <Preference xmlns:android="http://schemas.android.com/apk/res/android" > -->
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
     <PreferenceCategory
      android:summary="Global settings"
      android:title="Globals" >
	  <EditTextPreference
	     android:key="stun_server_ip"
	     android:summary="STUN server IP address"
	     android:title="STUN IP"
	     android:defaultValue="89.29.122.60"
	     />
	  <EditTextPreference
	     android:key="stun_server_port"
	     android:summary="STUN server UDP port"
	     android:title="STUN UDP port"
	     android:inputType="number"
	     android:defaultValue="3478"
	     />
   </PreferenceCategory>

   <PreferenceCategory
      android:summary="Random NAT related misc settings"
      android:title="Random NAT" >
	  <EditTextPreference
	     android:key="port_sleep"
	     android:summary="Sleep time in ms after 1 port scan"
	     android:title="Port sleep"
	     android:inputType="number"
	     android:defaultValue="0"
	     />
	  <EditTextPreference
	     android:key="random_portCount"
	     android:summary="Number of dst port to iterate over in Random"
	     android:title="Random PortCount"
	     android:inputType="number"
	     android:defaultValue="99"
	     />

 </PreferenceCategory>
 
 <PreferenceCategory
      android:summary="Symmetric NAT traversal related settings"
      android:title="Symmetric NAT" >
      <EditTextPreference
         android:key="symmetric_nat_traverse_ports"
         android:summary="How many ports try to open in traversal algorithm"
         android:title="Port number"
         android:inputType="number"
         android:defaultValue="25"
         />
  
 </PreferenceCategory>
    
    

</PreferenceScreen>
